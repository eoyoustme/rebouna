    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/Cease%20remake", delay = 325},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/Ripper", delay = 150},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/reband", delay = 210},
    {url = "https://pastefy.app/1xxpTxo3/raw", delay = 100000},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/Shocker", delay = 35},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/SIlence", delay = 300},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/HC%20Ambush", delay = 360},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/HC%20Ambush", delay = 600},
}

for _, entity in ipairs(entityList) do
    coroutine.wrap(function()
        while true do
            wait(entity.delay)
            pcall(function()
                loadstring(game:HttpGet(entity.url))()
            end)
        end
    end)()
end
