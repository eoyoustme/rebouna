require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("Hardcore Remake v2 has active ", true)
wait(2)
require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game).caption("real hardcore make by by noonie and ping this is my remake v2 version", true)

pcall(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vct0721/Doors-Stuff/refs/heads/main/Sprint.lua"))()
end)

local entityList = {
    {url = "in problem", delay = 325},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/Ripper", delay = 150},
    {url = "https://raw.githubusercontent.com/eoyoustme/Monoxide/refs/heads/main/A-60%20(hardcore%20but%20bad)", delay = 600},
    {url = "https://pastefy.app/1xxpTxo3/raw", delay = 100000},
    {url = "https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/Ripper", delay = 35},
    {url = "https://raw.githubusercontent.com/eoyoustme/Monoxide/refs/heads/main/Silence%20but%20bad", delay = 300},
}

for _, entity in ipairs(entityList) do
    coroutine.wrap(function()
        while true do
            wait(entity.delay)
            game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
            wait(0)
            pcall(function()
                loadstring(game:HttpGet(entity.url))()
            end)
        end
    end)()
end
