local deer = game:GetObjects("rbxassetid://86055776714285")[1]
deer.Parent = workspace
deer.PrimaryPart = deer:FindFirstChildWhichIsA("BasePart")
deer.PrimaryPart.Anchored = true
deer.PrimaryPart.CanCollide = false
