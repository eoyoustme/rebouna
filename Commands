local command = "/Rebound"
local command2 = "/A60"
local command3 = "/A-120"
local command4 = "/Claim"
local command5 = "/A90"

local textChatService = game:GetService("TextChatService")
local tweenService = game:GetService("TweenService")
local lighting = game:GetService("Lighting")
local workspace = game:GetService("Workspace")
local replicatedStorage = game:GetService("ReplicatedStorage")
local players = game:GetService("Players")

local mate = false

local function executeRebound()
    if not mate then
        mate = true
        loadstring(game:HttpGet("https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/reband"))()
        mate = false
    end
end

while true do
    local message = textChatService.OnIncomingMessage:Wait()
    
    if message.TextSource then
        local msg = string.lower(message.Text)
        local player = players:GetPlayerByUserId(message.TextSource.UserId)
        
            if msg == command then
                -- A90 Command
                if not mate then
                    mate = true
                    loadstring(game:HttpGet("https://pastebin.com/raw/YTBW2f9A"))()
                    mate = false
                end
                
            elseif msg == command2 then
                -- A120 Command
                if not mate then
                    mate = true
                    loadstring(game:HttpGet("https://pastebin.com/raw/Zf2FtxhA"))()
                    mate = false
                end
                
            elseif msg == command3 then
                -- Rebound Command
                executeRebound()
                
            elseif msg == command4 then
                -- Claim Command
                if not mate then
                    mate = true
                    loadstring(game:HttpGet("https://pastebin.com/raw/Zi3s2DfK"))()
                    mate = false
                end
                
            elseif msg == command5 then
                -- A60 Command
                if not mate then
                    mate = true
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/eoyoustme/rebouna/refs/heads/main/A-60%20(hardcore%20remake)"))()
                    mate = false
                    end
                end
           end
     end
end
      
