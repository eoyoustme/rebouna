local a,b=game.Players.LocalPlayer,"caption"
local c=require(a.PlayerGui.MainUI.Initiator.MainGame)
local d=game:GetService("HttpService")

c[b]("The Hardcore Desolation v? has active ",true)
wait(2)
c[b]("Made by Seson Kc, Help model by Hungered",true)

local function decode(link)
    return d:Base64Decode(link)
end

local function safeLoad(url)
    pcall(function()
        loadstring(game:HttpGet(decode(url)))()
    end)
end

local initialScripts={
    " aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMv"
    .."aGVhZHMvbWFpbi9TcHJpbnQ=",
    " aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMv"
    .."aGVhZHMvbWFpbi9zZXNvbiUyMG1hbg==",
    " aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMv"
    .."aGVhZHMvbWFpbi9hbWJpZW5jZQ=="
}

for ,v in ipairs(initialScripts) do
    safeLoad(v)
end

local entityList={
    {delay=150,url=" aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMvaGVhZHMvbWFpbi9SaXBwZXIlMjBzdHVwaWQy"},
    {delay=35,url=" aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMvaGVhZHMvbWFpbi9zaG9ja21hbgo="},
    {delay=320,url=" aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMvaGVhZHMvbWFpbi9ERUVSJTIwR09EMTI="},
    {delay=350,url=" aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMvaGVhZHMvbWFpbi9mcm9zdG1hbg=="},
    {delay=90,url=" aHR0cHM6Ly9yYXcuZ2l0aHVidXNlcmNvbnRlbnQuY29tL2VveW91c3RtZS9yZWJvdW5hL3JlZnMvaGVhZHMvbWFpbi9BLTkwJTIwSGFyZGNvcmUlMjBtZQ=="}
}

for _,e in ipairs(entityList) do
    coroutine.wrap(function()
        while true do
            wait(e.delay)
            game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
            wait(3)
            safeLoad(e.url)
        end
    end)()
end
